<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.infrastructure.mapper.primary.PrimaryAsyncDateTimeMapper">
    <insert id="insert" parameterType="java.time.LocalDateTime">
        INSERT INTO async_primary (registered_at, registered_id)
        values (#{dateTime}, "primary")
    </insert>
</mapper>